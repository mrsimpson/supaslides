{
  "name": "supaslides",
  "version": "0.0.1",
  "description": "Monorepo for Supaslides",
  "keywords": [
    "slidev",
    "presentations"
  ],
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "prepare": "node .husky/install.mjs"
  },
  "author": "mrsimpson",
  "license": "ISC",
  "devDependencies": {
    "husky": "9.1.7",
    "sass": "1.83.4",
    "turbo": "1.13.4"
  }
}
